pipeline {
    agent any

    stages {

        

        stage('Build Docker Image') {
            steps {
                script {
                    bat "docker build -t yash/mycontainer ."
                }
            }
        }

        stage('Run Docker Container') {
            steps {
                script {
                    bat "docker run -d --name my-app yash/container"
                }
            }
        }
    }
}
